<?xml version="1.0"?>
<classes>

    <class name="Gene" is-interface="true">
        <collection name="rnaseqExpressions" referenced-type="RnaSeqExpression" reverse-reference="gene" />
    </class>

    <class name="Transcript" is-interface="true">
        <collection name="rnaseqExpressions" referenced-type="RnaSeqExpression" reverse-reference="transcript" />
    </class>

    <class name="Experiment" is-interface="true">
        <attribute name="title" type="java.lang.String" />
        <attribute name="category" type="java.lang.String" />
        <reference name="dataSet" referenced-type="DataSet" reverse-reference="experiments" />
        <collection name="properties" referenced-type="SampleProperty" reverse-reference="experiments"/>
    </class>

<class name="RNASeqExpression" is-interface="true">
  <attribute name="TPM" type="java.lang.Double" />
  <attribute name="type" type="java.lang.String" />
  <reference name="gene" referenced-type="Gene" reverse-reference="RNASeqExpressions" />
  <reference name="transcript" referenced-type="Transcript" reverse-reference="RNASeqExpressions" />
  <reference name="study" referenced-type="Experiment" />
  <reference name="organism" referenced-type="Organism" />
</class>

    <class name="SampleProperty" is-interface="true">
        <attribute name="name" type="java.lang.String"/>
        <attribute name="value" type="java.lang.String"/>
        <collection name="experiments" referenced-type="Experiment" reverse-reference="properties" />
</class>

</classes>
