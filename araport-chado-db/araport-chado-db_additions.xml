<?xml version="1.0"?>
<classes>
  <class name="DataSetReference" extends="DataSet" is-interface="true" >
  <attribute name="urlPrefix" type="java.lang.String"/>
  <collection name="datasetCrossReferences" referenced-type="DataSetCrossReference" reverse-reference="datasetReference"/>
 </class>

<class name="DataSetCrossReference" is-interface="true" >
  <attribute name="primaryIdentifier" type="java.lang.String"/>
  <attribute name="externalIdentifier" type="java.lang.String"/>
  <reference name="subject" referenced-type="BioEntity" reverse-reference="datasetCrossReference"/>
  <attribute name="description" type="java.lang.String"/>
  <attribute name="version" type="java.lang.String"/>
  <reference name="datasetReference" referenced-type="DataSetReference" reverse-reference="datasetCrossReferences"/>
  <reference name="metadata" referenced-type="Metadata" reverse-reference="datasetCrossReference"/>
</class>

<class name="Metadata" is-interface="true" >
  <attribute name="primaryIdentifier" type="java.lang.String"/>
  <attribute name="createdTime" type="java.util.Date" />
  <attribute name="lastTimeUpdated" type="java.util.Date" />
  <attribute name="lastTimeModified" type="java.util.Date" />
  <attribute name="sourceOfOriginCreatedTime" type="java.util.Date" />
  <attribute name="sourceOfOriginLastModifiedTime" type="java.util.Date" />
  <attribute name="sourceOfOriginDateReleased" type="java.util.Date" />
  <reference name="datasetCrossReference" referenced-type="DataSetCrossReference" reverse-reference="metadata"/>
  <reference name="sourceloadedFrom" referenced-type="Source" reverse-reference="metadataReferences"/>
</class>


<class name="Source" is-interface="true" >
  <attribute name="name" type="java.lang.String"/>
  <attribute name="type" type="java.lang.String"/>
  <attribute name="sourceUri" type="java.lang.String"/>
  <collection name="metadataReferences" referenced-type="Metadata" reverse-reference="sourceloadedFrom"/>
</class>
 
<class name="BioEntity" is-interface="true" >
  <reference name="datasetCrossReference" referenced-type="DataSetCrossReference" reverse-reference="subject" />
</class>

<class name="DataEntity" is-interface="true" >
  <attribute name="primaryIdentifier" type="java.lang.String"/>
  <reference name="datasetCrossReference" referenced-type="DataSetCrossReference" />
</class>

</classes>
