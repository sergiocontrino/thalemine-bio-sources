	</class>
	
	<class name="AlleleClass" extends="CVTerm" is-interface="true">
		<reference name="vocabulary" referenced-type="AlleleClassCV"/>
	</class>
	
	<class name="Mutagen" extends="CVTerm" is-interface="true">
		<reference name="vocabulary" referenced-type="MutagenCV"/>
	</class>
	
	<class name="MutationSite" extends="CVTerm" is-interface="true">
		<reference name="vocabulary" referenced-type="MutationSiteCV"/>
	</class>
	
	<class name="InheritanceMode" extends="CVTerm" is-interface="true">
