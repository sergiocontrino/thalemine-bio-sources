<?xml version="1.0"?>
<classes>
		
	<class name="ControlledVocabulary" extends="Ontology" is-interface="true">
		<attribute name="uniqueName" type="java.lang.String" />
		<reference name="datasetReference" referenced-type="DataSetReference" />
		<collection name="cvTerms" referenced-type="CVTerm"
			reverse-reference="controlledVocabulary" />
	</class>
	
	<class name="CVTerm" extends="OntologyTerm" is-interface="true">
		<attribute name="uniqueName" type="java.lang.String" />
		<reference name="controlledVocabulary" referenced-type="ControlledVocabulary" reverse-reference="cvTerms"/>
	</class>
	
	<class name="AlleleClassCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
		<collection name="terms" referenced-type="AlleleClass"
			reverse-reference="vocabulary" />
	</class>
	
		
	<class name="InheritanceModeCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
			<collection name="terms" referenced-type="InheritanceMode"
			reverse-reference="vocabulary" />
	</class>
	
	<class name="MutationSiteCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
			<collection name="terms" referenced-type="MutationSite"
			reverse-reference="vocabulary" />
	</class>
	
	<class name="SequenceAlterationCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
			<collection name="terms" referenced-type="SequenceAlterationType"
			reverse-reference="vocabulary" />
	</class>
	
	<class name="MutagenCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
		<collection name="terms" referenced-type="Mutagen"
			reverse-reference="vocabulary" />
	</class>
	
	<class name="StockCategoryCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
			<collection name="terms" referenced-type="StockCategory"
			reverse-reference="vocabulary" />
	</class>
	
	<class name="StockTypeCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
		<collection name="terms" referenced-type="StockType"
			reverse-reference="vocabulary" />
	</class>
	
	<class name="StrainTypeCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
		
		<collection name="terms" referenced-type="StrainType"
			reverse-reference="vocabulary" />
		
	</class>
	
	<class name="ZygosityTypeCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
		
		<collection name="terms" referenced-type="ZygosityType"
			reverse-reference="vocabulary" />
			
	</class>
	
	<class name="CountryCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
		
	</class>
	
	<class name="RegionCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
	</class>
	
	<class name="GeodeticDatumCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
	</class>
	
	<class name="ContactTypeCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
		
		<collection name="terms" referenced-type="ContactType"
			reverse-reference="vocabulary" />
			
	</class>
	
	<class name="AttributionTypeCV" extends="ControlledVocabulary" is-interface="true">
		<reference name="datasetReference" referenced-type="DataSetReference" />
		<collection name="terms" referenced-type="AttributionType"
			reverse-reference="vocabulary" />
	</class>
	
	<!-- CV Terms -->
	
	<class name="StockType" extends="CVTerm" is-interface="true">
		<reference name="vocabulary" referenced-type="StockTypeCV"/>
	</class>
	
	<class name="StockCategory" extends="CVTerm" is-interface="true">
		<reference name="vocabulary" referenced-type="StockCategoryCV"/>
	</class>
	
	<class name="StrainType" extends="CVTerm"  is-interface="true">
