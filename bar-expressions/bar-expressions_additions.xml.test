<?xml version="1.0"?>

<classes>
	<class name="Experiment" is-interface="true">
		<attribute name="project" type="java.lang.String" />
		<attribute name="name" type="java.lang.String" />
		<attribute name="id" type="java.lang.Integer" />
		<attribute name="pi" type="java.lang.String" />
		<attribute name="institute" type="java.lang.String" />
		<collection name="samples" referenced-type="Sample"
			reverse-reference="experiment" />
	</class>
	<class name="Sample" is-interface="true">
		<attribute name="name" type="java.lang.String" />
		<attribute name="alias" type="java.lang.String" />
		<attribute name="fileName" type="java.lang.String" />
		<reference name="publication" referenced-type="Publication" />
		<reference name="organism" referenced-type="Organism" />
		<reference name="experiment" referenced-type="Experiment" />
    <collection name="data" referenced-type="SampleData"
      reverse-reference="sample" />
	</class>

	<class name="SampleProperty" is-interface="true">
    <attribute name="type" type="java.lang.String" />
		<attribute name="name" type="java.lang.String" />
    <attribute name="value" type="java.lang.String" />
		<collection name="samples" referenced-type="Sample"
			reverse-reference="properties" />
	</class>

	<class name="Tissue" extends="SampleProperty" is-interface="true">
		<attribute name="species" type="java.lang.String" />
		<attribute name="sex" type="java.lang.String" />
		<attribute name="organismPart" type="java.lang.String" />
	</class>

  <class name="SampleData" is-interface="true">
    <attribute name="signal" type="java.lang.Double" />
    <attribute name="call" type="java.lang.String" />
    <attribute name="pValue" type="java.lang.Double" />
    <reference name="sample" referenced-type="Sample"
      reverse-reference="data" />
  </class>


</classes>
